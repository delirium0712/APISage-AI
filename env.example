# Environment variables for the LangChain Agent Orchestration System

# Database Configuration
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://user:pass@localhost/agentdb

# LLM Configuration
OLLAMA_HOST=http://localhost:11434
LLM_MODEL=llama3:8b
USE_LOCAL_LLM=false
PRIMARY_LLM_PROVIDER=openai

# Vector Store Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
MILVUS_HOST=localhost
MILVUS_PORT=19530

# System Configuration
MAX_CONCURRENT_AGENTS=10
ENABLE_TRACING=true
ENABLE_CACHING=true

# =============================================================================
# EMBEDDING MODEL CONFIGURATION
# =============================================================================
# Default embedding model (Hugging Face)
EMBEDDING_MODEL=BAAI/bge-large-en-v1.5

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=4000

# Optional: LlamaCpp model path
LLAMA_MODEL_PATH=./models/llama-3-8b.gguf
